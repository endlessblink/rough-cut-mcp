{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "es2018"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}